<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE RDF [
   <!ENTITY dcterms "http://purl.org/dc/terms">  
   <!ENTITY owl "http://www.w3.org/2002/07/owl">  
   <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns">  
   <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema">  
   <!ENTITY xs "http://www.w3.org/2001/XMLSchema">    
   <!ENTITY rulemm "http://docs.oasis-open.org/legalruleml/ns/v1.0/rule-metamodel">  
   <!ENTITY lrmlmm "http://docs.oasis-open.org/legalruleml/ns/v1.0/metamodel">  
 ]>
<rdf:RDF   xmlns:owl="&owl;#" 
  xmlns:dc="&dcterms;/"
  xmlns:rdf="&rdf;#" xmlns:rdfs="&rdfs;#" xmlns:xs="&xs;"
  xmlns:rulemm="&rulemm;#" xmlns:lrmlmm="&lrmlmm;#" xml:base="&lrmlmm;">
  <!--<owl:Ontology rdf:about="&lrmlmm;/modules/upper.rdfs" dc:title="The LegalRuleML MetaModel Legal Source vocabulary"/>-->
  
    
  <rdfs:Class rdf:about="#Source">
    <rdfs:label>source</rdfs:label>
    <rdfs:comment>a source formulated in any format.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Associable"/>
    <rdfs:subClassOf rdf:resource="#Collectable"/>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="#Sources">
    <rdfs:label>source collection</rdfs:label>
    <rdfs:comment>a collection where every member is a Source.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Collection"/>
    <rdfs:subClassOf rdf:resource="#Collector"/>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="#LegalSource">
    <rdfs:label>legal source</rdfs:label>
    <rdfs:comment>a source of one or more Legal Norms formulated in any format and endorsed by an Authority.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Source"/>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="#LegalSources">
    <rdfs:label>legal source collection</rdfs:label>
    <rdfs:comment>a collection where every member is a LegalSource.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Sources"/>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="#Reference">
    <rdfs:label>reference</rdfs:label>
    <rdfs:comment>a pair consisting of an internal ID and an enriched non-IRI identifier, where the non-IRI is paired with some additional information that is sufficient to disambiguate the non-IRI to a unique Source.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Collectable"/>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="#References">
    <rdfs:label>reference collection</rdfs:label>
    <rdfs:comment>a collection where every member is a Reference.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Collection"/>
  </rdfs:Class>
      
  <rdfs:Class rdf:about="#LegalReference">
    <rdfs:label>legal reference</rdfs:label>
    <rdfs:comment>a Reference whose Source is a LegalSource.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Reference"/>
  </rdfs:Class>
  
  <rdfs:Class rdf:about="#LegalReferences">
    <rdfs:label>legal reference collection</rdfs:label>
    <rdfs:comment>a collection where every member is a LegalReference.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#References"/>
  </rdfs:Class>
  
  <rdf:Property rdf:about="#hasSources">
    <rdfs:label>has legal source collection</rdfs:label>
    <rdfs:comment>Ta collection of LegalSources.</rdfs:comment>
    <rdfs:range rdf:resource="#Sources"/>
    <rdfs:subPropertyOf rdf:resource="#hasCollection"/>    
  </rdf:Property>
  
  <rdf:Property rdf:about="#hasSource">
    <rdfs:label>has member source</rdfs:label>
    <rdfs:comment>a Source in the collection.</rdfs:comment>
    <rdfs:domain rdf:resource="#Sources"/>
    <rdfs:range rdf:resource="#Source"/>
    <rdfs:subPropertyOf rdf:resource="#hasMember"/>    
  </rdf:Property>
  
  <rdf:Property rdf:about="#hasLegalSource">
    <rdfs:label>has member legal source</rdfs:label>
    <rdfs:comment>a LegalSource in the collection of LegalSources.</rdfs:comment>
    <rdfs:domain rdf:resource="#LegalSources"/>
    <rdfs:range rdf:resource="#LegalSource"/>
    <rdfs:subPropertyOf rdf:resource="#hasSource"/>    
  </rdf:Property>
  
  <rdf:Property rdf:about="#hasLegalSources">
    <rdfs:label>has legal source collection</rdfs:label>
    <rdfs:comment>Ta collection of LegalSources.</rdfs:comment>
    <rdfs:range rdf:resource="#LegalSources"/>
    <rdfs:subPropertyOf rdf:resource="#hasSources"/>    
  </rdf:Property>
  
  <rdf:Property rdf:about="#hasReference">
    <rdfs:label>has member reference</rdfs:label>
    <rdfs:comment>a Reference in the collection.</rdfs:comment>
    <rdfs:domain rdf:resource="#References"/>
    <rdfs:range rdf:resource="#Reference"/>
    <rdfs:subPropertyOf rdf:resource="#hasMember"/>    
  </rdf:Property>

  <rdf:Property rdf:about="#hasReferences">
    <rdfs:label>has reference collection</rdfs:label>
    <rdfs:comment>a collection of References.</rdfs:comment>
    <rdfs:domain rdf:resource="#LegalRuleMLDocument"/>
    <rdfs:range rdf:resource="#References"/>
    <rdfs:subPropertyOf rdf:resource="#hasCollection"/>    
  </rdf:Property>
  
  <rdf:Property rdf:about="#hasLegalReference">
    <rdfs:label>has member legal reference</rdfs:label>
    <rdfs:comment>a LegalReference in the collection.</rdfs:comment>
    <rdfs:domain rdf:resource="#LegalReferences"/>
    <rdfs:range rdf:resource="#LegalReference"/>
    <rdfs:subPropertyOf rdf:resource="#hasReference"/>    
  </rdf:Property>
  
  <rdf:Property rdf:about="#hasLegalReferences">
    <rdfs:label>has legal reference collection</rdfs:label>
    <rdfs:comment>a collection of LegalReferences.</rdfs:comment>
    <rdfs:domain rdf:resource="#LegalRuleMLDocument"/>
    <rdfs:range rdf:resource="#LegalReferences"/>
    <rdfs:subPropertyOf rdf:resource="#hasReferences"/>    
  </rdf:Property>
    
  <rdf:Property rdf:about="#memberReferenceType">
    <rdfs:label>has member reference type</rdfs:label>
    <rdfs:comment>the conceptual type of references contained by the References collection).</rdfs:comment>
    <rdfs:domain rdf:resource="#References"/>
    <rdfs:range rdf:resource="&rdfs;#Class"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="#refersTo">
    <rdfs:label>refersTo</rdfs:label>
    <rdfs:comment>the internal ID of the Reference.</rdfs:comment>
    <rdfs:domain rdf:resource="#Reference"/>
    <rdfs:range rdf:resource="#Source"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="#refID">
    <rdfs:label>reference ID</rdfs:label>
    <rdfs:comment>the external ID of the Reference.</rdfs:comment>
    <rdfs:domain rdf:resource="#Reference"/>
    <rdfs:range rdf:resource="&xs;#string"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="#refIDSystemName">
    <rdfs:label>reference ID system name</rdfs:label>
    <rdfs:comment>the name of the ID system of the Reference (or of References contained by the References collection).</rdfs:comment>
    <rdfs:domain rdf:resource="#Reference"/>
    <rdfs:range rdf:resource="&xs;#string"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="#refIDSystemSource">
    <rdfs:label>reference ID system source</rdfs:label>
    <rdfs:comment>the IRI source of the ID system of the Reference (or of References contained by the References collection).</rdfs:comment>
    <rdfs:domain rdf:resource="#Reference"/>
    <rdfs:range rdf:resource="#Source"/>
  </rdf:Property>
  
  <rdf:Property rdf:about="#appliesSource">
    <rdfs:label>applies source</rdfs:label>
    <rdfs:comment>a Source or Reference applied by the Context or Association.</rdfs:comment>
    <rdfs:range rdf:resource="#Source"/>
    <rdfs:subPropertyOf rdf:resource="#applies"/>
  </rdf:Property>
  
  <!-- Duplicate for diagramming purposes -->
  <rdfs:Class rdf:about="#LegalRuleMLDocument">
    <rdfs:subClassOf rdf:resource="#Collector"/>
  </rdfs:Class>
  
  <!-- Duplicate from context.rdfs for diagramming purposes -->
  <rdf:Property rdf:about="#applies">
    <rdfs:domain rdf:resource="#Associator"/>
    <rdfs:range rdf:resource="#Associable"/>
  </rdf:Property>
  
</rdf:RDF>
