dc:language [ "en" ]
start |= notAllowed

# Common Patterns
## common attribute group for LegalRuleML elements
commonLRMLInit.attlist &= empty
## common attribute group for LegalRuleML Nodes
commonLRMLNodeInit.attlist &= commonLRMLInit.attlist?
commonLRMLNodeInit.attlist &= NodeLabel.attrib.choice?
commonLRMLNodeInit.attlist &= NodeReference.attrib.opt.choice?
commonLRMLNodeInit.attlist &= type.lrml.attrib.choice?

## common attribute group for LegalRuleML edges
commonLRMLEdgeInit.attlist &= commonLRMLInit.attlist?
commonLRMLEdgeInit.attlist &= xml-id.choice?

##
iri.lrml.attrib.opt.choice |= iri.lrml.attrib.def?
iri.lrml.attrib.choice |= iri.lrml.attrib.def
iri.lrml.attrib.def =
  
  ## an IRI providing details regarding the parent Node Element.
  attribute iri { iri.lrml.datatype }

##
type.lrml.attrib.choice |= type.lrml.attrib.def
type.lrml.attrib.def =
  
  ## the type or class of the parent Node Element.
  attribute type { iri.lrml.datatype }

##
DummyCommon = element DummyCommon { commonLRMLNodeInit.attlist, iri.lrml.attrib.opt.choice?, iri.lrml.datatype }
