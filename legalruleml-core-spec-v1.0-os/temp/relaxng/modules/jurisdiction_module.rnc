dc:language [ "en" ]
start |= JurisdictionCollection.Node.def | Jurisdiction.Node.def

##
JurisdictionCollection.Node.choice |= JurisdictionCollection.Node.def

##
JurisdictionCollection.Node.def =
  
  ## a collection where every member is a Jurisdication.
  element Jurisdictions { JurisdictionCollection.type.def }
JurisdictionCollection.type.def = JurisdictionCollection.attlist, JurisdictionCollection.main

##
JurisdictionCollection.attlist &= commonCollection.attlist?

##
Jurisdiction.Node.choice |= Jurisdiction.Node.def
Jurisdiction.Node.def =
  
  ## a geographic area or subject-matter over which an Authority applies its legal power.
  element Jurisdiction { Jurisdiction.type.def }
Jurisdiction.type.def = Jurisdiction.attlist, Jurisdiction.main

##
Jurisdiction.attlist &= commonLRMLNodeInit.attlist?
Jurisdiction.attlist &= sameAs.attrib.opt.choice?
