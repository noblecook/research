namespace dc = "http://purl.org/dc/elements/1.1/"
namespace dcterms = "http://purl.org/dc/terms/"

dc:title [ "LegalRuleML Document" ]
dc:version [ "1.0" ]
dc:creator [ "Tara Athan (taraathan AT gmail.com)" ]
dc:contributor [ "Harold Boley" ]
dc:contributor [ "Guido Governatori" ]
dc:contributor [ "Monica Palmirani" ]
dc:contributor [ "Adrian Paschke" ]
dc:contributor [ "Adam Wyner" ]
dc:subject [ "LegalRuleML, document" ]
dc:description [ "The expansion module for LegalRuleML documents." ]
dc:language [ "en" ]
start |= LegalRuleML

##
LegalRuleML =
  
  ## a formal representation of one or more LegalSources using the LegalRuleML Specifications.
  element LegalRuleML { LegalRuleML.type.def }

## type of the LegalRuleML element  
LegalRuleML.type.def = LegalRuleMLDocument.attlist, LegalRuleMLDocument.main

## attributes of the LegalRuleML element
LegalRuleMLDocument.attlist &= commonLRMLNodeInit.attlist?
LegalRuleMLDocument.attlist &= xml-base.choice?
LegalRuleMLDocument.attlist &= hasCreationDate.attrib.opt.choice?
LegalRuleMLDocument.attlist &= xsi-schemalocation.choice? 

##
LegalRuleMLDocument.main |=
  hasComment.edge.choice* & 
  (hasPrefix.edge.choice*,
  ((hasLegalReferenceCollection.edge.choice |
  hasLegalSourceCollection.edge.choice |
  hasReferenceCollection.edge.choice |
  hasSourceCollection.edge.choice |
  hasTimeCollection.edge.choice |
  hasTemporalCharacteristics.edge.choice |
  hasAgentCollection.edge.choice |
  hasFigureCollection.edge.choice |
  hasRoleCollection.edge.choice |
  hasAuthorityCollection.edge.choice |
  hasJurisdictionCollection.edge.choice )*,
  hasAssociationCollection.edge.choice*,
  (hasAlternativeCollection.edge.choice |
  hasContext.edge.choice |
  hasStatementCollection.edge.choice)*))

